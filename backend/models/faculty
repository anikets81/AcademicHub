const mongoose = require('mongoose');

const facultySchema = mongoose.Schema({
    facultyId:{
        type:String,
        required:true
    },
    name: {
        type: String,
        required: true
        // minlength: 5,
        // maxlength: 20
    },
    password: {
        type: String,
        required: true,
        // minlength: 6
    },
    email: {
        type: String,
        required: true,
        unique: true,
        // minlength: 5,
        // maxlength: 30
    },
    phone: {
        type: Number,
        required: true,
        unique: true,
        // length: 10
    }
},{timestamps: true});

module.exports = mongoose.model('faculty',facultySchema);